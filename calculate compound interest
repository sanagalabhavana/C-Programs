//write a c program to calculate compound interest
#include <stdio.h>
#include <math.h>
int main(){
    float p,r,t;
    printf("Enter principal : ");
    scanf("%f",&p);
    printf("\n Enter rate : ");
    scanf("%f",&r);
    printf("\n Enter time :");
    scanf("%f",&t);
    
    float CI=p*pow((1+r/100),t);
    printf("\n Compound Interest : %f",CI);
    return 0;
}
